<h2>Celebrities</h2>
<!-- <a href="/celebrity/new">New Celebrity</a> -->
<form action="/celebrity/new" method="get">
  <button type="submit">New Celebrity</button>
</form>
<br>
<ul>
  {{#each celebrities}}
    <a href="/celebrity/{{_id}}">{{name}}</a>
    <!-- As part of the loop, add a <form> tag that makes a POST request to celebrities/:id/delete where the :id is replaced by the actual id of each celebrity.
 -->
    <form action="celebrity/{{_id}}/delete" method="post">
      <button type="submit">Delete</button>
    </form>

  {{/each}}
</ul>
